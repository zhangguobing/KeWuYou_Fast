<resources>

    <!-- SettingView -->
    <declare-styleable name="SettingView">
        <attr name="iOSStyle" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SettingViewItem">
        <attr name="setting_view_title" format="string" />
        <attr name="setting_view_subTitle" format="string" />
        <attr name="setting_view_titleColor" format="reference" />
        <attr name="setting_view_subTitleColor" format="reference" />
        <attr name="setting_view_titleSize" format="reference|dimension" />
        <attr name="setting_view_subTitleSize" format="reference|dimension" />
        <attr name="setting_view_drawable" format="reference" />
        <attr name="setting_view_arrow" format="reference" />
        <attr name="setting_view_check" format="reference" />
        <attr name="setting_view_image" format="reference" />
        <attr name="setting_view_clickable" format="boolean" />
        <attr name="setting_view_checked" format="boolean" />
        <attr name="setting_view_background" format="reference" />
    </declare-styleable>
    <!-- SettingView -->

</resources>